<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>Elthelas Campaign Setting News</title>
    <link>https://elthelas.com/blog</link>
    <description>News and changes concerning the Elthelas Campaign Setting for Dungeons and Dragons</description>
    {% for item in items %}
      <item>
        <title>{{ item.title }}</title>
        <link>{{ item.link }}</link>
        <description><![CDATA[ {{ item.description | raw }} ]]></description>
      </item>
    {% endfor %}
  </channel>
</rss>